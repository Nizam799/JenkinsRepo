pipeline {
 agent none
 stages {
  stage ('Test') {
  agent { docker { image 'node:16-alpine' } }
   steps {
    sh 'node --version'
    }
   }
  stage ('build') {
  agent { docker { image 'node:16-alpine' } }
   steps {
    sh 'git clone "https://github.com/Nizam799/JenkinsRepo.git" /home/snp/'
    }
   }
 }
}
